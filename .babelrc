{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "> 0.5%"
          ],
          "node": 6
        }
      }
    ],
  ],
  "ignore": [
    "**/*.test.js"
  ],
  "retainLines": true,
  "sourceMaps": true,
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ]
}